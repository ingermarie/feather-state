{
  "name": "feather-state",
  "version": "1.1.3",
  "description": "A feather light state framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "pack": "tsup --config config/tsup.config.ts",
    "min": "tsup --config config/tsup.config.min.ts && gzip -c build/index.min.global.js | wc -c",
    "build": "npm run pack && npm run min",
    "test": "tsup tests/tests.ts --outDir build && node build/tests.js"
  },
  "author": "ingermarien@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "terser": "^5.26.0",
    "tsup": "^8.0.1"
  }
}
